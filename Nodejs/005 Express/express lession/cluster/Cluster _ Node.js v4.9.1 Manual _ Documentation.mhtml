@charset "utf-8";

html { -webkit-font-smoothing: antialiased; font-variant-ligatures: none; }

body { font-family: Lato, "Lucida Grande", "Lucida Sans Unicode", "Lucida Sans", Verdana, Tahoma, sans-serif; font-size: 62.5%; margin: 0px; padding: 0px; color: rgb(51, 51, 51); background: rgb(255, 255, 255); }

h1, h2, h3, h4 { margin: 0.8em 0px 0.5em; line-height: 1.2; }

h5, h6 { margin: 1em 0px 0.8em; line-height: 1.2; }

h1 { margin-top: 0px; font-size: 2.441em; }

h2 { font-size: 1.953em; }

h3 { font-size: 1.563em; }

h4 { font-size: 1.25em; }

h5 { font-size: 1em; }

h6 { font-size: 0.8em; }

pre, tt, code, .pre, span.type, a.type { font-family: Monaco, Consolas, "Lucida Console", monospace; }

#content { font-size: 1.8em; position: relative; }

a, a:link, a:active { color: rgb(67, 133, 61); text-decoration: none; border-radius: 2px; padding: 0.1em 0.2em; margin: -0.1em; }

a:hover, a:focus { color: rgb(255, 255, 255); background-color: rgb(67, 133, 61); outline: none; }

strong { font-weight: 700; }

code a:hover { background: none; }

em code { font-style: normal; }

#changelog #gtoc { display: none; }

#gtoc { font-size: 0.8em; }

.line { width: calc(100% - 1em); display: block; padding-bottom: 1px; }

.api_stability { margin: 0px 0px 1em; font-family: Lato, "Lucida Grande", "Lucida Sans Unicode", "Lucida Sans", Verdana, Tahoma, sans-serif; font-weight: 700; color: white !important; }

.api_stability * { color: white !important; }

.api_stability a { text-decoration: underline; }

.api_stability a:hover, .api_stability a:active, .api_stability a:focus { background: rgba(255, 255, 255, 0.4); }

.api_stability a code { background: none; }

.api_stability_0 { background-color: rgb(214, 0, 39); }

.api_stability_1 { background-color: rgb(236, 83, 21); }

.api_stability_2 { background-color: rgb(78, 186, 15); }

.api_stability_3 { background-color: rgb(0, 132, 182); }

.api_metadata { font-size: 0.75em; margin-bottom: 1em; }

.api_metadata span { margin-right: 1em; }

.api_metadata span:last-child { margin-right: 0px; }

ul.plain { list-style: none; }

abbr { border-bottom: 1px dotted rgb(69, 69, 69); }

p { position: relative; text-rendering: optimizeLegibility; margin: 0px 0px 1.125em; line-height: 1.5em; }

#apicontent > :last-child { margin-bottom: 0px; padding-bottom: 2em; }

table { border-collapse: collapse; margin: 0px 0px 1.5em; }

th, td { border: 1px solid rgb(170, 170, 170); }

th { text-align: left; }

ol, ul, dl { margin: 0px 0px 0.6em; padding: 0px; }

ol ul, ol ol, ol dl, ul ul, ul ol, ul dl, dl ul, dl ol, dl dl { margin-bottom: 0px; }

ul, ol { margin-left: 2em; }

dl dt { position: relative; margin: 1.5em 0px 0px; }

dl dd { position: relative; margin: 0px 1em; }

dd + dt.pre { margin-top: 1.6em; }

h1, h2, h3, h4, h5, h6 { text-rendering: optimizeLegibility; font-weight: 700; position: relative; }

header h1 { font-size: 2em; line-height: 2em; margin: 0px; }

#apicontent { padding-top: 1em; }

#apicontent .line { width: calc(50% - 1em); margin: 1em 1em 0.95em; background-color: rgb(204, 204, 204); }

h2 + h2 { margin: 0px 0px 0.5em; }

h3 + h3 { margin: 0px 0px 0.5em; }

h2, h3, h4, h5 { position: relative; padding-right: 40px; }

h1 span, h2 span, h3 span, h4 span { position: absolute; display: block; top: 0px; right: 0px; }

h1 span:hover, h2 span:hover, h3 span:hover, h4 span:hover { opacity: 1; }

h1 span a, h2 span a, h3 span a, h4 span a { font-size: 0.8em; color: rgb(0, 0, 0); text-decoration: none; font-weight: bold; }

pre, tt, code { line-height: 1.5em; margin: 0px; padding: 0px; }

.pre { line-height: 1.5em; font-size: 1.2em; }

pre { padding: 1em; vertical-align: top; background: rgb(242, 242, 242); margin: 1em; overflow-x: auto; }

pre > code { font-size: 0.8em; padding: 0px; }

pre + h3 { margin-top: 2.225em; }

code.pre { white-space: pre; }

#intro { margin-top: 1.25em; margin-left: 1em; }

#intro a { color: rgb(221, 221, 221); font-size: 1.25em; font-weight: bold; }

hr { background: none; border-width: medium medium 1px; border-style: none none solid; border-top-color: initial; border-right-color: initial; border-left-color: initial; border-image: initial; border-bottom-color: rgb(122, 122, 122); margin: 0px 0px 1em; }

#toc h2 { font-size: 1em; line-height: 0; margin: 1.5em 0px; }

#toc ul { font-size: 0.8125em; }

#toc ul ul { font-size: 1em; }

#toc ul a { text-decoration: none; }

#toc ul li { margin-bottom: 0.666em; list-style: square outside; }

#toc li > ul { margin-top: 0.666em; }

#toc .stability_0::after { background-color: rgb(213, 0, 39); color: rgb(255, 255, 255); }

#toc .stability_0::after { content: "deprecated"; font-size: 0.8em; position: relative; top: -0.18em; left: 0.5em; padding: 0px 0.3em 0.2em; border-radius: 3px; }

#apicontent li { margin-bottom: 0.5em; }

#apicontent li:last-child { margin-bottom: 0px; }

tt, code { font-size: 0.9em; color: rgb(4, 4, 4); background-color: rgb(242, 242, 242); border-radius: 2px; padding: 0.1em 0.3em; }

a code { color: inherit; background: inherit; padding: 0px; }

.type { font-size: 0.9em; line-height: 1.5em; }

#column1.interior { margin-left: 234px; padding: 0px 2em; -webkit-padding-start: 1.5em; }

#column2.interior { width: 234px; background: rgb(51, 51, 51); position: fixed; left: 0px; top: 0px; bottom: 0px; overflow-x: hidden; overflow-y: scroll; }

#column2 ul { list-style: none; margin: 0.9em 0px 0.5em; background: rgb(51, 51, 51); }

#column2 > :first-child { margin: 1.25em 1em; }

#column2 > ul:nth-child(2) { margin: 1.25em 0px 0.5em; }

#column2 > ul:last-child { margin: 0.9em 0px 1.25em; }

#column2 ul li { padding-left: 1.4em; margin-bottom: 0.5em; padding-bottom: 0.5em; font-size: 0.8em; }

#column2 .line { margin: 0px 0.5em; background-color: rgb(112, 112, 112); }

#column2 ul li:last-child { margin-bottom: 0px; }

#column2 ul li a { color: rgb(204, 204, 204); border-radius: 0px; }

#column2 ul li a.active, #column2 ul li a.active:hover, #column2 ul li a.active:focus { color: rgb(67, 133, 61); border-radius: 0px; border-bottom: 1px solid rgb(67, 133, 61); background: none; }

#intro a:hover, #intro a:focus, #column2 ul li a:hover, #column2 ul li a:focus { color: rgb(255, 255, 255); background: none; }

span > .mark, span > .mark:visited { font-size: 1em; color: rgb(112, 112, 112); position: absolute; top: 0px; right: 0px; }

span > .mark:hover, span > .mark:focus, span > .mark:active { color: rgb(67, 133, 61); background: none; }

th, td { padding: 0.75em 1em; vertical-align: top; }

th > :last-child, td > :last-child { margin-bottom: 0px; }

.clearfix::after { content: "."; display: block; height: 0px; clear: both; visibility: hidden; }

@media only screen and (max-width: 1024px) {
  #content { font-size: 2.5em; overflow: visible; }
  #column1.interior { margin-left: 0px; padding-left: 0.5em; padding-right: 0.5em; width: auto; overflow-y: visible; }
  #column2 { display: none; }
}

@media only screen and (max-width: 1024px) and (orientation: portrait) {
  #content { font-size: 3.5em; }
}

@media print {
  html { height: auto; }
  #column2.interior { display: none; }
  #column1.interior { margin-left: auto; overflow-y: auto; }
}
