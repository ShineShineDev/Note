.cta { display: flex; background: url("/docs/static/img/cta-background.png") 0px 65% / cover, url("/docs/static/img/cta-background-preload.png"), rgb(69, 25, 50); position: relative; color: white; width: 100%; margin: 0px; padding-top: 1rem; padding-bottom: 1rem; min-height: 200px; transition-property: opacity, height, min-height, padding; transition-duration: 300ms; transform: translate3d(0px, 0px, 0px); backface-visibility: hidden; overflow: hidden; z-index: 10; }

