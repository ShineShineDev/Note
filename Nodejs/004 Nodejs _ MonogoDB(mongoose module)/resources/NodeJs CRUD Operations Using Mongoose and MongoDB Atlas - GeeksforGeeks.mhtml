@charset "utf-8";

:root { --font-sofia:sofia-pro,sans-serif; --font-din:"urw-din",sans-serif; --transition-time:0.4s; --color-gfg:#308D46; --color-black:#273239; --gfg-body-color:white; --gfg-tint-color:white; --gfg-selected-color:#ececec; --gfg-title-color:#121212; --gfg-subtitle-color:#121212; --gfg-tags-color:#989898; --gfg-text-color:#989898; --gfg-color-lg:#6d6d6d; --gfg-grey-bg:#F9F9F9; --gfg-darkgrey:#ebebeb; --gfg-light-bg:#D7EFEE; --gfg-tags-base-color:#308D46; --color-gfg-bg:rgba(48, 141, 70, 0.1); --gfg-wrapper-bg:#FAFAFA; --font-section-head:27.5pt; --font-section-subhead:14pt; --font-article-head:14pt; --font-article-subtext:12pt; --font-article-text:10pt; --font-article-tags:10pt; --font-w-article-head:600; --font-w-article-text:400; --image-grid:url(https\:\/\/media\.geeksforgeeks\.org\/wp-content\/cdn-uploads\/icon-grid-3-20210405\.svg); }

.material-icons { font-family: "Material Icons"; font-weight: 400; font-style: normal; font-size: 24px; line-height: 1; letter-spacing: normal; text-transform: none; display: inline-block; white-space: nowrap; word-wrap: normal; direction: ltr; font-feature-settings: "liga"; -webkit-font-smoothing: antialiased; }

.eventLink { border: 1px solid rgb(134, 134, 134); background-color: rgb(224, 224, 224); padding: 10px; box-shadow: none; display: block; text-align: center; font-size: 14px; font-weight: 700; color: rgb(85, 85, 85) !important; }

#gfg-side-sticky-tab { display: none; position: fixed; top: 60vh; left: 50px; z-index: 9999; }

#gfg-side-sticky-tab div { margin: 0px; padding: 0px; }

#gfg-side-sticky-tab .sticky-container { transform: rotate(-90deg); border-radius: 0px 0px 6px 6px; left: -190px; position: fixed; transition-property: left; transition-duration: 0.6s; width: 180px; }

#gfg-side-sticky-tab .sticky-container:hover { left: -10px; }

#gfg-side-sticky-tab .gfg-callout { position: relative; display: block; border-radius: 0px 0px 6px 6px; }

#gfg-side-sticky-tab .slideout { position: relative; vertical-align: middle; height: 180px; display: block; background-color: rgb(255, 255, 255); transform: rotate(90deg); overflow-y: scroll; direction: rtl; outline: rgb(204, 204, 204) solid 1px; }

#gfg-side-sticky-tab .slideout .inner { text-align: center; }

#gfg-side-sticky-tab .slideout .inner .inner-item { padding: 10px 0px 12px 10px; font-size: 14px; }

#gfg-side-sticky-tab .slideout .inner .inner-item a { color: rgb(64, 64, 64); text-decoration: none; }

#gfg-side-sticky-tab .slideout .inner .inner-item .hand-write-icon { font-size: 4rem; }

#gfg-side-sticky-tab .slideout .inner .inner-item a:hover { color: rgb(0, 0, 0); }

#gfg-side-sticky-tab .gfg-callout .slide-out-btn { color: rgb(255, 255, 255); background-color: var(--color-gfg); height: 40px; width: 41px; margin: 0px; padding: 0px; display: block; text-align: center; font-size: 20px; font-weight: 600; border-radius: 0px 0px 6px 6px; position: absolute; right: -1px; border: 0px; cursor: pointer; }

.cookie-consent { width: 100%; flex-wrap: wrap; position: fixed; bottom: 0px; left: 0px; right: 0px; color: rgb(214, 214, 214); background-color: rgb(60, 64, 77); padding: 0.6em 1.8em; z-index: 9999; display: none; }

.cookie-text { font-family: Helvetica, Calibri, Arial, sans-serif; font-size: 12px; line-height: 1.5em; flex: 99 1 500px; text-align: center; margin: 6px 30px 5px 0px; }

.cookie-text a { text-decoration: none; color: rgb(169, 165, 165) !important; }

.cookie-text a:hover { color: rgb(214, 214, 214) !important; }

.consent-btn { margin-right: 30px; color: rgb(0, 0, 0); flex: 1 0 130px; background-color: rgb(139, 237, 79); border-color: transparent; padding: 0.2em 0.5em; font-size: 1.2em; font-family: Helvetica, Calibri, Arial, sans-serif; font-weight: bolder; border-width: 2px; border-style: solid; text-align: center; white-space: nowrap; border-radius: 5px; min-height: 30px; }

.consent-btn:hover { background-color: rgb(177, 255, 117); cursor: pointer; text-decoration: underline; }

.show-consent { display: -webkit-flex; }

.lightbox-target { position: fixed; top: 0px; bottom: 0px; z-index: -1; background: rgba(0, 0, 0, 0.7); width: 100%; opacity: 0; transition: 0.5s ease-in-out; overflow: hidden; }

.lightbox-target img { margin: auto; position: absolute; top: 0px; left: 0px; right: 0px; bottom: 0px; max-height: 0%; max-width: 0%; border: 3px solid rgb(255, 255, 255); box-shadow: rgba(0, 0, 0, 0.3) 0px 0px 8px; box-sizing: border-box; transition: 0.5s ease-in-out; }

span.lightbox-close { display: block; width: 50px; height: 50px; border-radius: 50%; box-sizing: border-box; background: rgb(255, 255, 255); color: rgb(0, 0, 0); text-decoration: none; position: absolute; cursor: pointer; transition: opacity 0.2s; top: 10px !important; right: 10px !important; }

span.lightbox-close:hover { opacity: 0.85; }

span.lightbox-close::before { content: ""; display: block; height: 30px; width: 1px; background: rgb(0, 0, 0); position: absolute; left: 25px; top: 10px; transform: rotate(45deg); }

span.lightbox-close::after { content: ""; display: block; height: 30px; width: 1px; background: rgb(0, 0, 0); position: absolute; left: 25px; top: 10px; transform: rotate(-45deg); }

.lightbox-display { opacity: 1 !important; z-index: 10000 !important; }

.lightbox-display-image { max-height: 80% !important; max-width: 100% !important; }

.lightbox-display-close { top: 0px !important; }

#practiceLinkDiv { font-size: 18px; font-weight: 700; margin: 24px 0px; }

i.material-icons { opacity: 0; width: 24px; transition: opacity 0.5s; }

.header--nav__link { font-size: 18px; color: rgb(83, 70, 70); text-shadow: none; cursor: pointer; text-decoration: none !important; }

.hideIt { display: none; }

.noteHeaderText { float: left; }

.hideNotesDivIcon { float: right; margin-bottom: -10px; }

.hideNotesDivIcon > i { font-size: 30px; margin-top: -5px; }

.hideNotesDivIcon > i:hover { cursor: pointer; box-shadow: rgb(255, 255, 255) 0px 0px 0px 1px; }

.saveNoteDiv { margin-top: -1px; }

.processSaveNote { float: left; margin-left: 5px; }

.tooltip { position: relative; display: inline-block; float: right; margin: -10px -10px -5px; }

.tooltip:not(.noInner) > i { font-size: 25px; background-color: var(--color-gfg); border-radius: 25%; color: rgb(255, 255, 255); }

.tooltip:not(.noInner) > i:hover { cursor: pointer; }

.tooltip .tooltiptext { visibility: hidden; width: 120px; background-color: rgb(85, 85, 85); color: rgb(255, 255, 255); text-align: center; border-radius: 6px; padding: 5px 0px; position: absolute; z-index: 1; bottom: 125%; left: 50%; margin-left: -60px; opacity: 0; transition: opacity 0.3s; }

.tooltip .tooltiptext::after { content: ""; position: absolute; top: 100%; left: 50%; margin-left: -5px; border-width: 5px; border-style: solid; border-color: rgb(85, 85, 85) transparent transparent; }

.tooltip:hover .tooltiptext { visibility: visible; opacity: 1; }

.emphasizeText { animation-name: emphasizeTexts; animation-duration: 1s; transition-delay: 1s; }

@keyframes emphasizeTexts { 
  0% { transform: scale(1); }
  50% { transform: scale(1.03); transition: 0.5s; }
  100% { transform: scale(1); transition: 0.5s; }
}

#loginSpanText { display: none; text-align: center; font-size: 24px; color: rgb(255, 255, 255); border: 2px solid rgba(32, 28, 28, 0.2); width: 100%; height: 100%; opacity: 0.9; z-index: 100; position: absolute; top: -3px; left: -3px; background-color: rgba(32, 28, 28, 0.2); }

#insideLoginSpanText { position: relative; top: 45%; }

#insideLoginSpanText > a { text-decoration: none; color: rgb(255, 255, 255); border: 2px solid rgba(32, 28, 28, 0.2); background-color: rgb(59, 177, 102); width: 100%; padding: 10px; }

#insideLoginSpanText > a:hover { cursor: pointer; }

@media (max-width: 600px) {
  .addNoteButton, .improveArticleButton { margin-top: 5px; font-size: 15px; }
}

.select-icon-5 { color: rgb(65, 140, 86); }

.select-icon-4 { color: rgb(98, 159, 11); }

.select-icon-3 { color: rgb(236, 217, 12); }

.select-icon-2 { color: rgb(243, 149, 9); }

.select-icon-1 { color: rgb(215, 52, 17); }

.select-icon-1, .select-icon-2, .select-icon-3, .select-icon-4, .select-icon-5 { transform: scale(1.15, 1.15); }

.modal { padding-top: 100px; display: none; position: fixed; z-index: 9999; left: 0px; width: 100%; height: 100%; overflow: auto; background-color: rgba(0, 0, 0, 0.4); top: 0px !important; }

.modal-content { background-color: rgb(254, 254, 254); margin: auto; padding: 20px; border: 1px solid rgb(136, 136, 136); width: 60%; }

.modal-header-style { text-align: center; background-color: var(--color-gfg); }

.tabsContent > h1 { font-weight: 500; color: rgb(255, 255, 255); font-size: 24px; background-color: rgb(65, 140, 86); text-align: center; padding: 20px; }

.modal-body-style { color: rgb(0, 0, 0); }

.modal-btn1 { background-color: rgb(106, 191, 100); border-color: rgb(219, 223, 219); border-style: solid; color: rgb(255, 255, 255); box-shadow: none; height: 36px; padding: 0px 20px; border-radius: 5%; }

.modal-btn2 { background-color: rgb(242, 54, 12); color: rgb(255, 255, 255); border-color: rgb(242, 54, 12); border-style: solid; margin-left: 30px; height: 36px; padding: 0px 20px; border-radius: 5%; }

.modal-btn1:hover, .modal-btn2:hover { cursor: pointer; box-shadow: rgb(170, 170, 170) 0px 3px 10px 0px; }

.ratingEmoji { margin-top: 20px; }

.ratingEmojis { text-align: center; }

.ratingEmojiHeader { font-size: 20px; text-align: center; }

.ratingEmojis > span > i { font-size: 70px; }

.ratingEmojis > span > i:hover { cursor: pointer; transform: scale(1.15, 1.15); }

#ratingIcon1:hover { color: rgb(215, 52, 17); }

#ratingIcon2:hover { color: rgb(243, 149, 9); }

#ratingIcon3:hover { color: rgb(236, 217, 12); }

#ratingIcon4:hover { color: rgb(98, 159, 11); }

#ratingIcon5:hover { color: rgb(65, 140, 86); }

.tags { text-align: center; font-size: 16px; margin-top: 6px; }

.commentTags { margin-top: 10px; }

.additionalComments { margin-top: 15px; }

.additionalCommentsHeader, .commentTagsHeader, .ratingEmojiHeader { text-align: center; font-size: 22px; }

.inputComment { text-align: center; margin-top: 5px; }

.additionalCommentStyle { width: 50%; overflow-y: scroll; resize: none; height: 60px; font-size: 110%; }

.modal-fixed-footer { text-align: right; }

a, abbr, acronym, address, applet, article, aside, audio, b, big, blockquote, body, canvas, caption, center, cite, code, dd, del, details, dfn, div, dl, dt, em, embed, fieldset, figcaption, figure, footer, form, h1, h2, h3, h4, h5, h6, header, hgroup, html, i, iframe, img, ins, kbd, label, legend, li, mark, menu, nav, object, ol, output, p, pre, q, ruby, s, samp, section, small, span, strike, strong, sub, summary, sup, table, tbody, td, tfoot, th, thead, time, tr, tt, u, ul, var, video { margin: 0px; padding: 0px; border: 0px; font-size: 100%; vertical-align: baseline; }

.entry-content li, .entry-content p, .entry-content pre, .entry-content table { font-size: 16px; text-align: left; line-height: 1.58; }

.entry-content pre { font-size: 15px; }

pre { background-color: rgb(224, 224, 224); border-radius: 10px; color: rgba(0, 0, 0, 0.9); font-family: Consolas, monospace; margin-bottom: 10px; overflow: auto; padding: 10px; }

body { line-height: 1; }

ol, ul { list-style: none; }

blockquote, q { quotes: none; }

blockquote::after, blockquote::before, q::after, q::before { content: none; }

table { border-collapse: collapse; border-spacing: 0px; }

caption, td, th { font-weight: 400; text-align: left; }

h1, h2, h3, h4, h5, h6 { clear: both; }

html { overflow-y: scroll; font-size: 100%; text-size-adjust: 100%; }

a:focus { outline: 0px; }

article, aside, details, figcaption, figure, footer, header, nav, section { display: block; text-align: justify; }

hgroup { display: inline-block; float: none; }

hgroup a { font-size: 4em; text-decoration: none; padding-left: 15px; color: var(--color-gfg); font-family: Georgia, "Times New Roman", Times, Helvetica, Arial, serif; font-weight: lighter; letter-spacing: -0.05em; }

