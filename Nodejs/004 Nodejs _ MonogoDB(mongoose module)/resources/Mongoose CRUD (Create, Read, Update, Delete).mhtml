  .site-header-background::before { background: rgba(0, 0, 0, 0.6); }
  .post-feed { background: var(--darkmode); }
  .post-card, .post-card:hover { border-bottom-color: rgb(23, 25, 28); }
  .author-profile-image { background: var(--darkmode); }
  .post-card-byline-content a { color: rgba(255, 255, 255, 0.75); }
  .post-card-byline-content a:hover { color: rgb(255, 255, 255); }
  .post-card-image { background: var(--darkmode); }
  .post-card-title { color: rgba(255, 255, 255, 0.85); }
  .post-card-excerpt { color: rgb(144, 162, 170); }
  .author-avatar, .static-avatar { border-color: rgb(26, 28, 32); }
  .page-template .site-main, .post-full-content, .post-template .site-main, .site-main { background: var(--darkmode); }
  .post-full-title { color: rgba(255, 255, 255, 0.9); }
  .post-full-custom-excerpt { color: rgb(144, 162, 170); }
  .post-full-image { background-color: rgb(23, 25, 28); }
  .post-full-byline { border-top-color: rgb(39, 42, 48); }
  .post-full-byline-meta h4 a { color: rgba(255, 255, 255, 0.75); }
  .post-full-byline-meta h4 a:hover { color: rgb(255, 255, 255); }
  .author-list-item .author-card { background: rgb(14, 15, 17); box-shadow: rgba(0, 0, 0, 0.4) 0px 12px 26px; }
  .author-list-item .author-card::before { border-top-color: rgb(14, 15, 17); }
  .no-image .author-social-link a { color: rgba(255, 255, 255, 0.75); }
  .post-full-content h1, .post-full-content h2, .post-full-content h3, .post-full-content h4, .post-full-content h6 { color: rgba(255, 255, 255, 0.9); }
  .post-full-content a { color: rgb(255, 255, 255); box-shadow: rgb(255, 255, 255) 0px -1px 0px inset; }
  .post-full-content em, .post-full-content strong { color: rgb(255, 255, 255); }
  .post-full-content code { color: rgb(255, 255, 255); background: rgb(0, 0, 0); }
  hr { border-top-color: rgb(23, 25, 28); }
  .post-full-content hr::after { background: rgb(23, 25, 28); box-shadow: var(--darkmode) 0 0 0 5px; }
  .post-full-content figcaption { color: rgba(255, 255, 255, 0.6); }
  .post-full-content table td:first-child { background-image: linear-gradient(90deg,var(--darkmode) 50%,rgba(5,5,6,0)); }
  .post-full-content table td:last-child { background-image: linear-gradient(270deg,var(--darkmode) 50%,rgba(5,5,6,0)); }
  .post-full-content table th { color: rgba(255, 255, 255, 0.85); background-color: rgb(23, 25, 28); }
  .post-full-content table td, .post-full-content table th { border: 1px solid rgb(23, 25, 28); }
  .post-full-content .kg-bookmark-container, .post-full-content .kg-bookmark-container:hover { color: rgba(255, 255, 255, 0.75); box-shadow: rgba(255, 255, 255, 0.9) 0px 0px 1px; }
  .post-full-content input { color: rgb(48, 58, 62); }
  .kg-bookmark-title { color: rgb(255, 255, 255); }
  .kg-bookmark-description, .kg-bookmark-metadata { color: rgba(255, 255, 255, 0.75); }
  .site-archive-header .no-image { color: rgba(255, 255, 255, 0.9); background: var(--darkmode); }
  .site-archive-header .no-image .site-header-content { border-bottom-color: rgb(39, 42, 48); }
  .site-header-content .author-profile-image { box-shadow: rgba(255, 255, 255, 0.04) 0px 0px 0px 6px; }
  .subscribe-form { border: none; background: linear-gradient(rgb(0, 0, 0), rgb(0, 0, 0)); }
  .subscribe-form-title { color: rgba(255, 255, 255, 0.9); }
  .subscribe-form p { color: rgba(255, 255, 255, 0.7); }
  .subscribe-email { border-color: rgb(18, 20, 23); color: rgba(255, 255, 255, 0.9); background: rgb(11, 12, 14); }
  .subscribe-email:focus { border-color: rgb(62, 67, 76); }
  .subscribe-form button { opacity: 0.9; }
  .subscribe-form .error .message-error, .subscribe-form .invalid .message-error { color: rgb(237, 105, 77); }
  .subscribe-form .success .message-success { color: rgb(171, 207, 81); }
