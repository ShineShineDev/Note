.native-inline { display: block; margin-top: 1.5em; padding: 10px 20px; border-radius: 4px; background: repeating-linear-gradient(-45deg, transparent, transparent 5px, rgba(0, 0, 0, 0.03) 5px, rgba(0, 0, 0, 0.03) 10px) rgba(0, 0, 0, 0.03); }

.native-inline a { display: block; text-decoration: none; line-height: 1.5; }

