.lists-v2 i { color: rgb(231, 76, 60); }

.process-v1 .process-in > li i { background: rgb(231, 76, 60); }

.featured-blog h2::after { background: rgb(231, 76, 60); }

.featured-blog .featured-img i:hover { color: rgb(255, 255, 255); background: rgb(231, 76, 60); }

.rgba-default { background-color: rgb(231, 76, 60); }

