@charset "utf-8";

.interactive-slider-v3 { z-index: 1; padding: 40px 0px; position: relative; background: url("../img/sliders/layer/bg2.jpg") center center / cover no-repeat; }

.interactive-slider-v3.img-v1 { background: url("../img/bg/25.jpg") center center / cover no-repeat; }

.interactive-slider-v3.img-v2 { background: url("../img/bg/14.jpg") center center / cover no-repeat; }

.interactive-slider-v3::after { top: 0px; left: 0px; z-index: -1; width: 100%; height: 100%; content: " "; position: absolute; background: rgba(0, 0, 0, 0.4); }

.interactive-slider-v3 h1 { text-align: center; color: rgb(255, 255, 255); font-size: 40px; font-weight: 600; line-height: 50px; text-transform: uppercase; }

.interactive-slider-v3 p { color: rgb(255, 255, 255); font-size: 24px; font-weight: 200; font-family: "Open Sans", Arial, sans-serif; }

@media (max-width: 768px) {
  .interactive-slider-v3 h1 { font-size: 20px; line-height: 30px; margin-bottom: 20px; }
  .interactive-slider-v3 p { color: rgb(255, 255, 255); font-size: 14px; font-weight: 200; font-family: "Open Sans", Arial, sans-serif; }
}

.sliderButton { text-align: center; }

.course-block { padding: 10px; text-align: center; margin-bottom: 10px; }

.course-block p, .course-block h2 { color: rgb(255, 255, 255); }

.course-block h2 a:hover { text-decoration: none; }

interactive-slider-v3 a:hover { text-decoration: none; }

.course-block-light, .course-block-default { background: rgb(250, 250, 250); border: 1px solid rgb(238, 238, 238); opacity: 0.8; }

.course-block-default:hover { box-shadow: rgb(238, 238, 238) 0px 0px 8px; background-color: rgb(227, 211, 185); }

.course-block-light p, .course-block-light h2, .course-block-default p, .course-block-default h2 { color: rgb(85, 85, 85); }

.course-slider a:hover { text-decoration: none; }

.row.display-flex { display: flex; flex-wrap: wrap; }

.row.display-flex > [class*="col-"] { flex-grow: 1; }

#relatedPosts .box { border: 1px solid rgb(102, 102, 102); height: 100%; }

.row.display-flex a:hover { text-decoration: none; }
