@charset "utf-8";

.banner-image { background-image: url("../img/bg/online-class-1.jpg"); background-color: rgb(204, 204, 204); height: 300px; background-position: center center; background-repeat: no-repeat; background-size: cover; line-height: 300px; text-align: center; }

.banner-text p { line-height: 1.5; display: inline-block; font-size: 180%; padding: 15px; color: white; vertical-align: middle; }

.banner-text img { margin-top: 10px; }

#banner.jumbotron { background-image: url("../img/bg/online-class-1.jpg") !important; }

#banner.jumbotron a:hover { text-decoration: none !important; }
