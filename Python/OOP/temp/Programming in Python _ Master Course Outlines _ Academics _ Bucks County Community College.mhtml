@media (max-device-width: 736px) {
  body.vd-noscroll { position: fixed !important; }
}

@media (min-width: 576px) {
  .vm-button { right: 1rem; left: auto; }
  .vm-wrapper { justify-content: center; }
  .vm-wrapper iframe { max-width: 770px; max-height: 600px; top: auto; opacity: 0; margin-top: 50px; position: static; }
  .vm-wrapper.vm-show { pointer-events: auto; background-color: rgba(0, 0, 0, 0.5); }
  .vm-wrapper.vm-show iframe { top: auto; opacity: 1; margin-top: 0px; }
  body.vd-noscroll { position: initial; }
}
