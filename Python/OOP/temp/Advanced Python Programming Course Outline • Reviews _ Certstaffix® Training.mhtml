Content-Location: https://www.certstaff.com/css/slimmenu.css

@charset "utf-8";

.menu-collapser { position: relative; background-color: transparent; color: rgb(255, 255, 255); text-shadow: rgba(0, 0, 0, 0.5) 0px 1px 0px; width: 100%; height: 48px; line-height: 48px; font-size: 14px; padding: 0px 8px; box-sizing: border-box; }

.collapse-button { position: absolute; right: 45px; top: 50%; width: 40px; background-color: transparent; background-image: linear-gradient(rgb(43, 67, 109), rgb(43, 67, 109)); background-repeat: repeat-x; border-color: rgba(0, 0, 0, 0.1) rgba(0, 0, 0, 0.1) rgba(0, 0, 0, 0.25); border-radius: 4px; border-style: solid; border-width: 1px; color: rgb(255, 255, 255); box-shadow: rgba(255, 255, 255, 0.1) 0px 1px 0px inset, rgba(255, 255, 255, 0.075) 0px 1px 0px; padding: 7px 10px; text-shadow: rgba(0, 0, 0, 0.25) 0px -1px 0px; cursor: pointer; font-size: 14px; text-align: center; transform: translate(0px, -50%); box-sizing: border-box; }

.collapse-button:hover, .collapse-button:focus { background-image: none; background-color: rgb(43, 67, 109); color: rgb(255, 255, 255); }

.collapse-button .icon-bar { background-color: rgb(245, 245, 245); border-radius: 1px; box-shadow: rgba(0, 0, 0, 0.25) 0px 1px 0px; display: block; height: 2px; width: 18px; margin: 2px 0px; }

ul.slimmenu { list-style-type: none; margin: 0px; padding: 0px; width: 100%; height: auto; }

ul.slimmenu li { position: relative; display: inline-block; background-color: transparent; }

ul.slimmenu > li { margin-right: -5px; border-left: 1px solid rgb(153, 153, 153); }

ul.slimmenu > li:first-child { border-left: 0px; }

ul.slimmenu > li:last-child { margin-right: 0px; }

ul.slimmenu li a { display: block; color: rgb(255, 255, 255); padding: 10px 34px 12px 16px; font-family: "Open Sans", sans-serif; font-size: 12px; font-weight: bold; text-shadow: rgba(255, 255, 255, 0.2) 0px 1px 0px; transition: background-color 0.5s ease-out 0s; }

ul.slimmenu li li a { width: 170px; height: auto; padding: 5px 5px 5px 10px; font-size: 11px; }

ul.slimmenu li li li a { width: 170px; height: auto; padding: 5px 5px 5px 10px; font-size: 11px; font-weight: normal; }

ul.slimmenu li a:hover { background-color: rgb(235, 186, 57); text-decoration: none; }

ul.slimmenu li .sub-collapser { background: none 0px 0px repeat scroll rgba(0, 0, 0, 0.075); position: absolute; right: 0px; top: 0px; width: 25px; height: 100%; text-align: center; z-index: 999; cursor: pointer; font-weight: normal; }

ul.slimmenu li .sub-collapser::before { content: ""; display: inline-block; height: 100%; vertical-align: middle; margin-right: -0.25em; }

ul.slimmenu li .sub-collapser > i { color: rgb(255, 255, 255); font-size: 12px; font-weight: normal; display: inline-block; vertical-align: middle; }

ul.slimmenu li ul { margin: 0px; list-style-type: none; }

ul.slimmenu li ul li { background-color: rgb(43, 67, 109); }

ul.slimmenu li > ul { display: none; position: absolute; left: 0px; top: 100%; z-index: 999; width: 100%; }

ul.slimmenu li > ul > li ul { display: none; position: absolute; left: 100%; top: 0px; z-index: 999; width: 100%; }

ul.slimmenu.collapsed li { display: block; width: 100%; height: auto; box-sizing: border-box; }

ul.slimmenu.collapsed li a { display: block; border-bottom: 1px solid rgba(0, 0, 0, 0.075); box-sizing: border-box; }

ul.slimmenu.collapsed li .sub-collapser { height: 40px; }

ul.slimmenu.collapsed li > ul { display: none; position: static; }
