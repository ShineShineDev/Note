@charset "utf-8";

#ddmenu { display: block; font-family: "Helvetica Neue", Helvetica, Arial, sans-serif; text-align: center; letter-spacing: normal; box-shadow: rgb(43, 67, 109) 0px 5px 12px 0px; position: relative; z-index: 100; }

#ddmenu ul { margin: 0px auto; padding: 2px 0px 0px; text-align: center; width: 100%; font-size: 0px; background: rgb(41, 68, 110); display: inline-block; list-style: none; position: relative; z-index: 999999990; border-radius: 2px; }

#ddmenu li { margin: 0px; padding: 0px; font-size: 14px; display: inline-block; zoom: 1; position: relative; color: rgb(255, 255, 255); line-height: 44px; vertical-align: middle; transition: background-color 0.2s ease 0s; outline: none; user-select: none; }

#ddmenu .full-width { position: static; }

#ddmenu .over { color: rgb(255, 255, 255); background-color: rgb(234, 185, 52); }

#ddmenu .over.no-sub { color: rgb(255, 255, 255); background-color: rgb(234, 185, 52); border-radius: 0px; }

#ddmenu .top-heading { font-weight: normal; margin: 0px 22px; color: inherit; text-decoration: none; display: inline-block; outline: 0px; cursor: pointer; }

#ddmenu a, #ddmenu a:link, #ddmenu a:hover { color: inherit; }

#ddmenu a:hover { text-decoration: underline; }

#ddmenu a:focus { outline: rgb(0, 153, 255) dotted 1px; }

#ddmenu .caret { color: inherit; left: -18px; width: 0px; height: 0px; overflow: hidden; vertical-align: middle; margin-bottom: 2px; border-top: 4px solid; border-right: 4px solid transparent; border-left: 4px solid transparent; display: inline-block; position: relative; }

#ddmenu .dropdown { width: auto; left: 0px; color: rgb(51, 51, 51); padding: 0px; margin: 0px; display: none; position: absolute; overflow: hidden; top: 100%; border-right: 1px solid rgb(187, 187, 187); border-bottom: 1px solid rgb(187, 187, 187); border-left: 1px solid rgb(187, 187, 187); border-image: initial; border-top: none; border-radius: 0px 0px 3px 3px; box-shadow: rgba(0, 0, 0, 0.176) 0px 6px 6px; }

#ddmenu .full-width .dropdown { width: 100%; padding: 0px; margin: 0px; }

#ddmenu .offset300 { left: -300px; right: auto; }

#ddmenu .right-aligned { left: auto; right: 0px; }

#ddmenu .over .dropdown { display: block; }

#ddmenu .dd-inner { text-align: center; padding: 20px; margin: 0px; background-color: rgb(243, 243, 243); white-space: nowrap; font-size: 13px; }

#ddmenu ul ul { margin: 0px; padding: 0px; text-align: left; width: auto; background: none; border: none; display: block; position: static; z-index: 0; border-radius: 0px; }

#ddmenu ul ul li { font-size: 13px; padding: 6px 0px; color: inherit; line-height: 1; margin: 0px; display: block; position: static; background: none; border: none; transition: none 0s ease 0s; border-radius: 0px; }

#ddmenu .dropdown a { color: rgb(41, 68, 110); line-height: 1; text-decoration: none; transition: color 0.4s ease 0s; }

#ddmenu .dropdown a:hover, #ddmenu .dropdown a:focus { text-decoration: underline; color: rgb(234, 185, 52); }

#ddmenu .column { text-align: left; vertical-align: top; display: inline-block; white-space: normal; width: auto; min-width: 200px; padding: 0px 30px; border-right: 1px solid rgb(51, 51, 51); }

#ddmenu .dd-inner ul:last-child { border-right: none; }

#ddmenu h3 { font-weight: 500; line-height: 1.1; margin-top: 12px; margin-bottom: 8px; font-size: 24px; }

#ddmenu li { border-radius: 3px 3px 0px 0px; }

#ddmenu h3 { text-transform: uppercase; border-bottom: 1px solid rgb(51, 51, 51); }

#ddmenuLink { display: none; }

#ddmenu .menu-icon { display: none; }

@media only screen and (max-width: 480px) {
  #ddmenu .column.mayHide { display: none; }
  #ddmenu ul { width: 100%; display: none; border-radius: 0px; }
  #ddmenu li { position: relative; display: block; font-size: 20px; text-align: left; border-bottom: 1px solid rgba(0, 0, 0, 0.2); border-radius: 0px; }
  #ddmenu .full-width { position: relative; }
  #ddmenu .dropdown { position: relative; border: none; border-radius: 0px; box-shadow: none; background-color: rgb(243, 243, 243); }
  #ddmenu .offset300 { left: 0px; }
  #ddmenu .full-width .dropdown { width: auto; }
  #ddmenu .dd-inner { margin: 0px; background: none; text-align: left; }
  #ddmenu .column { width: auto; padding: 0px; border: none; display: block; }
  #ddmenu .caret { position: absolute; left: auto; right: 14px; top: 19px; border-width: 5px; }
  #ddmenu .menu-icon { font-size: 32px; display: block; position: relative; width: 40px; height: 36px; text-align: center; cursor: pointer; user-select: none; background-color: rgb(255, 255, 255); border-radius: 3px; margin-bottom: 6px; margin-left: 12px; margin-right: auto; }
  #ddmenu .menu-icon::before { content: ""; position: absolute; top: 0.25em; left: 4px; width: 1em; height: 0.125em; border-top: 0.375em double rgb(41, 68, 110); border-bottom: 0.125em solid rgb(41, 68, 110); box-sizing: content-box; }
  #ddmenu .menu-icon-active { background-color: rgb(41, 68, 110); }
  #ddmenu .menu-icon-active::before { border-color: rgb(255, 255, 255); }
}
