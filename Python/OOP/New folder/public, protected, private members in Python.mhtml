  .topMenuHorizontal .navbar-nav.left .nav-item { display: none; }
  .topMenuHorizontal .navbar-nav.left .nav-item:nth-last-of-type(1), .topMenuHorizontal .navbar-nav.left .nav-item:nth-of-type(1) { display: block; }
  .topMenuHorizontal .navbar-nav.nav-flex.right .nav-item:nth-of-type(3) { display: none; }
  .navigation .btn { width: 98px; font-size: 15px; }
  .lb-ad { min-height: 300px; display: block; }
  .inart-ad { min-height: 300px; display: block; }
  .phHeader .navbar { height: 40px; }
  .devmed-lb, .tt-ad-rect, .tt-ad-skyscraper { display: none; }
  .tt-ad-halfpage { display: none; }
  .tt-ad-squre2 { display: none; }
  .tt-ad-header { width: 300px; height: 50px; display: block; }
  .collapse.navbar-collapse { padding: 0px; }
}

@media (max-width: 556px) {
  .navbar-brand img { width: 90%; }
  .card-header { font-size: 18px; }
  .navigation .btn { width: 98px; font-size: 14px; }
  .copy-btn { display: none; }
}

@media (max-width: 480px) {
  input[type="text"]#q:focus { max-width: 240px; }
}
