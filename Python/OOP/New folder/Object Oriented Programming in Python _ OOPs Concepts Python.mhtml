@charset "utf-8";

header.fixed #sale-strip { display: none; }

#sale-strip { display: flex; align-items: center; justify-content: center; width: 100%; color: rgb(255, 255, 255); background: rgb(238, 61, 36); padding: 8px 0px; }

#sale-strip a { text-decoration: none; font-size: 16px; color: rgb(255, 255, 255); }

#sale-strip #clockdiv { display: flex; }

#sale-strip #clockdiv span { display: inline-block; margin: 0px 10px; font-size: 16px; color: rgb(255, 255, 255); position: relative; }

#sale-strip #clockdiv span sup { position: absolute; top: 5px; right: -14px; font-weight: 700; font-size: 14px; }

#sale-strip #clockdiv h4 { font-size: 18px; margin: 0px; color: rgb(255, 255, 255); }

.strip-btn { padding: 0.25px 1rem; font-size: 14px; background: rgb(255, 255, 255); border: 1px solid rgb(255, 255, 255); margin-left: 10px; border-radius: 30px; color: rgb(255, 0, 0) !important; }

@media only screen and (max-width: 768px) {
  .strip-btn { margin-left: 0px; }
}

.strip-btn:hover { background: rgb(255, 255, 255); }

.strip-btn:focus { background: rgb(255, 255, 255); }

#sale-strip p { justify-content: center; margin: 0px; }

#sale-strip .btn { display: block; }

@media only screen and (max-width: 991px) {
  #sale-strip { flex-direction: column; padding: 10px 15px; }
  #sale-strip #clockdiv { margin-top: 10px; }
}
