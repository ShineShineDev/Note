@keyframes example { 
  0% { color: rgb(255, 215, 0); transform: rotate(0deg); }
  100% { font-size: 48px; color: rgb(255, 215, 0); transform: rotate(360deg); }
}

@keyframes tutpoint { 
  0% { background-color: transparent; color: transparent; }
  1% { background-color: rgb(76, 175, 80); color: rgb(255, 255, 255); }
  5% { font-size: 16px; }
