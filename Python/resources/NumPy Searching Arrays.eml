@charset "utf-8";

*, ::after, ::before { box-sizing: border-box; }

body, body * { position: absolute; vertical-align: baseline; border: 0px none; outline: 0px; padding: 0px; margin: 0px; cursor: pointer; }

#main_wrapper { position: absolute; display: block; width: 300px; height: 250px; overflow: hidden; background-color: rgb(255, 255, 255); opacity: 0; }

.active#main_wrapper { opacity: 1; }

#frame { content: ""; border: 1px solid rgb(0, 0, 0); position: absolute; width: 100%; height: 100%; top: 0px; left: 0px; z-index: 10000; pointer-events: none; }

#top_part { overflow: hidden; width: 300px; height: 250px; }

#bg_top { background: url("img/bg_top.jpg") 0% 0% / 300px 250px; width: 300px; height: 250px; }

#bg_bottom { background: url("img/bg_bottom.jpg") 0% 0% / 300px 250px; width: 300px; height: 250px; }

#logo-starbucks { position: absolute; background-image: url("img/Starbucks_logo.png"); background-size: 300px 250px; width: 300px; height: 250px; pointer-events: none; }

#hide-all { position: absolute; width: 100%; height: 100%; background-color: rgb(255, 255, 255); z-index: 5000; }

#CTA_wrapper { position: absolute; top: 214px; left: 184px; width: 101px; height: 25px; pointer-events: none; overflow: hidden; }

#cta_over_wrapper { left: -12px; transform: skewX(153.5deg); position: absolute; width: 0px; height: 25px; pointer-events: none; overflow: hidden; }

#cta_normal { position: absolute; width: 101px; height: 25px; background-image: url("img/cta_normal.png"); background-size: 101px 25px; pointer-events: none; }

#cta_over { left: 12px; transform: skewX(26.5deg); position: absolute; width: 101px; height: 25px; background-image: url("img/cta_over.png"); background-size: 101px 25px; pointer-events: none; }
